{
  "name": "@adobe/aio-lib-files",
  "version": "1.0.5",
  "description": "An abstraction on top of blob cloud storage exposing a file like API",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint index.js lib test",
    "beautify": "npm run lint -- --fix",
    "test": "npm run unit && npm run lint",
    "unit": "jest -c ./jest.config.js",
    "e2e": "jest -c ./jest.e2e.config.js",
    "jsdoc": "jsdoc2md -f index.js 'lib/**/*.js' > doc/api.md",
    "typings": "jsdoc -t node_modules/tsd-jsdoc/dist -r lib -d .",
    "generate-docs": "npm run jsdoc && npm run typings"
  },
  "author": "Adobe Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": "adobe/aio-lib-files",
  "keywords": [
    "sdk",
    "openwhisk",
    "storage",
    "cloud-native",
    "adobe-io"
  ],
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsdoc": "^24.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.2",
    "node-fetch": "^2.6.0",
    "tsd-jsdoc": "^2.4.0"
  },
  "dependencies": {
    "@adobe/aio-lib-core-errors": "^3.0.0",
    "@adobe/aio-lib-core-logging": "1.1.0",
    "@adobe/aio-lib-core-tvm": "^1.0.1",
    "@azure/storage-blob": "^10.5.0",
    "@hapi/joi": "^16.1.7",
    "@types/hapi__joi": "^16.0.1",
    "fs-extra": "^9.0.0",
    "lodash.clonedeep": "^4.5.0",
    "mime-types": "^2.1.24",
    "upath": "^1.2.0"
  }
}
